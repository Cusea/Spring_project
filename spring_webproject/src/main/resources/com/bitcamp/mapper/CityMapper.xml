	PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bitcamp.mapper.CityMapper">
	<select id="selectList" resultType="com.bc.web_project.vo.CityVo">
		SELECT * FROM city
	</select>
	<select id="selectOne" resultType="com.bc.web_project.vo.CityVo">
		SELECT * FROM CITY WHERE NUM=#{num}
	</select>
</mapper>
